type Query {
  books: [Book]
  authors: [Author]
  bookById(id: ID!): Book,
  authorById(id: ID!): Author,
}

type Mutation {
  addAuthor(addAuthorDTO: AddAuthorDTO!): Author,
  editAuthor(id: ID!, editAuthorDTO: EditAuthorDTO!): Author
  addBook(addBookDTO: AddBookDTO!): Book,
  editBook(id: ID!, editBookDTO: EditBookDTO!): Book,
}